{% extends 'base.html.twig' %}

{% block body %}
    <h1>Decryptedvotes list</h1>

    <table>
        <thead>
            <tr>
                <th>Decryptedvalue</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for decryptedvote in decryptedvotes %}
            <tr>
                <td><a href="{{ path('decryptedvote_show', { 'id': decryptedvote.id }) }}">{{ decryptedvote.decryptedValue }}</a></td>
                <td>{{ decryptedvote.id }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('decryptedvote_show', { 'id': decryptedvote.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('decryptedvote_edit', { 'id': decryptedvote.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('decryptedvote_new') }}">Create a new decryptedvote</a>
        </li>
    </ul>
{% endblock %}
